.container{
  display: flex;
  flex-direction: row;
  justify-content: center;

  width: 100%;

}

.two{
  
    width: 375rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.item1{
    width: 320rpx;
    height: 350rpx;
    background: linear-gradient(to top,#ffecd2,#fcb69f);
    display: flex;
    flex-direction: column;
    margin-bottom: 50rpx;
    border-radius: 20rpx;
    justify-content: center;
}

.item2{
   width: 320rpx;
   height: 630rpx;
   background: linear-gradient(to top,#a8d0e6 60%,#e7f0fd);
   display: flex;
   flex-direction: column;
   border-radius: 20rpx;
   justify-content: center;
}

.item3{
   width: 320rpx;
   height: 630rpx;
   background: linear-gradient(to top,#fdfcfb,#e2d1c3);
   display: flex;
   flex-direction: column;
   margin-bottom: 50rpx;
   border-radius: 20rpx;
   justify-content: center;
}

.item4{
   width: 320rpx;
   height: 350rpx;
   background-color: #CDDCDC;
 background:linear-gradient( #dfe9f3,#ffffff);
   display: flex;
   flex-direction: column;
   justify-content: center;
   border-radius: 20rpx;

}

.word1{
  font-size: 30rpx;
  color: #464646;
  margin-left: 10rpx;
}

