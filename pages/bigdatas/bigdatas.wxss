.container{
  background: #f7f5f9;
  width: 100%;
  height: 100%;
  padding: 50rpx 0;
}


.item{
  display: flex;
  flex-direction: column;
}


.photo{
   width: 600rpx;
   height: 250rpx;
   border-radius: 15rpx 15rpx 0 0;
   vertical-align: bottom;
}

.detail{
  border-radius: 0 0 15rpx 15rpx;
  background: white;
  padding: 15rpx 0;
  font-size: 25rpx;
  text-align: center;
}